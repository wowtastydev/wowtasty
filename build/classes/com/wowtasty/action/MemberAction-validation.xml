<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"  
       "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="mvo.email">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message key="errors.required" />
		</field-validator>
		<field-validator type="email">
			<message key="errors.invalid" />
		</field-validator>
	</field>
	<field name="memberPasswordStr">
		<field-validator type="requiredstring">
			<message key="errors.required" />
		</field-validator>
	</field>
	<field name="confirmPasswordStr">
		<field-validator type="requiredstring">
			<message key="errors.required" />
		</field-validator>
	</field>
	<field name="mvo.telephone">
		<field-validator type="regex">
			<param name="expression">^[0-9- ]+$</param>
			<message key="errors.invalid" />
		</field-validator>
	</field>
	<field name="mvo.suite">
		<field-validator type="regex">
			<param name="expression">^[a-zA-Z0-9]+$</param>
			<message key="errors.invalid" />
		</field-validator>
	</field>
	<field name="mvo.postalCode">
		<field-validator type="regex">
			<param name="expression">^[a-zA-Z0-9 ]+$</param>
			<message key="errors.invalid" />
		</field-validator>
	</field>
	<field name="mvo.delivTelephone">
		<field-validator type="regex">
			<param name="expression">^[0-9- ]+$</param>
			<message key="errors.invalid" />
		</field-validator>
	</field>
	<field name="mvo.delivSuite">
		<field-validator type="regex">
			<param name="expression">^[a-zA-Z0-9]+$</param>
			<message key="errors.invalid" />
		</field-validator>
	</field>
	<field name="mvo.delivBuzzer">
		<field-validator type="regex">
			<param name="expression">^[a-zA-Z0-9]+$</param>
			<message key="errors.invalid" />
		</field-validator>
	</field>
	<field name="delivPostalCode">
		<field-validator type="regex">
			<param name="expression">^[a-zA-Z0-9 ]+$</param>
			<message key="errors.invalid" />
		</field-validator>
	</field>
</validators>